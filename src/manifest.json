{
   "manifest_version": 2,
   "name": "Secret",
   "version": "0.5",
   "description": "Extension for SECRET",
   "permissions": [
      "activeTab",
      "declarativeContent",
      "storage",
      "contextMenus"
   ],
   "background": {
      "page":"background/background.html",
      "persistent": false
   },
   "browser_action": {
     "default_popup": "popup/popup.html",
     "default_icon": "icon.png"
   }

}
 