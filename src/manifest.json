{
   "manifest_version": 2,
   "name": "Secret",
   "version": "0.5",
   "description": "Build an Extension!",
   "permissions": ["activeTab", "declarativeContent", "storage"],
   "background": {
     "scripts": ["background.js"],
     "persistent": false
   },
   "browser_action": {
     "default_popup": "popup/popup.html",
     "default_icon": "icon.png"
   }
}
 